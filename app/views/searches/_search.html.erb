
<div id="searches-pertial">
  <%= form_with url: searches_path, method: :get, local: true do | f | %>
  <%= f.text_field :keyword, placeholder: 'フリーワード検索' %>
  <%= f.select :model, { 'User' => 'User', 'Book' => 'Book' } %>
  <%= f.select :requirement, { '完全一致' => 'all', '前方一致' => 'front', '後方一致' => 'back', '部分一致' => 'pertial' } %>
  <%= f.submit '検索' %>
  <% end %>
</div>
